<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.nozel.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_cari"
        android:name="com.example.nozel.ui.cari.CariFragment"
        android:label="Cari"
        tools:layout="@layout/fragment_cari" />

    <fragment
        android:id="@+id/navigation_perpus"
        android:name="com.example.nozel.ui.perpus.PerpusFragment"
        android:label="Perpus"
        tools:layout="@layout/fragment_perpus" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.nozel.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>